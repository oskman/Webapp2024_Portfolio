generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Project {
  id            String    @id @default(uuid())
  name          String
  description   String
  startDate     DateTime?
  endDate       DateTime?
  technologies  String
  repositoryURL String
  status        String
  isPublic      Boolean   @default(true)
}
